{
  "name": "Super Github",
  "version": "1.0.7",
  "description": "Enhance your GitHub experience",
  "manifest_version": 3,
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://github.com/*"
      ],
      "js": [
        "super-github.js"
      ],
      "run_at": "document_end"
    }
  ]
}